{
  "name": "@stinobe/git-tools",
  "version": "0.0.0",
  "description": "A tool to help validate commit messages before committing & branch names before pushing them.",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "ts-patch install -s"
  },
  "bin": {
    "stinobe-commit-message": "./lib/branch.js",
    "stinobe-branch-name": "./lib/commits.js"
  },
  "keywords": [
    "git",
    "validator",
    "commit",
    "message",
    "branch",
    "name"
  ],
  "author": {
    "name": "Stinobe",
    "url": "https://stino.be/"
  },
  "repository": "github:@stinobe/git-tools",
  "license": "none",
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "ci-info": "^3.3.0"
  }
}